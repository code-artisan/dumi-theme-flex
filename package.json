{
  "name": "dumi-theme-flex",
  "version": "0.0.7",
  "description": "dumi-theme-flex",
  "keywords": [
    "dumi",
    "dumi-theme",
    "chakra",
    "mobile"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/code-artisan/dumi-theme-flex.git",
    "branch": "main"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "run-p dev:*",
    "build": "run-s build:*",
    "dev:father": "father dev",
    "dev:example": "pnpm -F ./example run start",
    "build:father": "father build",
    "build:example": "pnpm -F ./example run build",
    "preview": "cd ./serve && deno task dev",
    "up:deps": "pnpm up",
    "prepare": "husky install && father link-dev-theme",
    "lint": "pnpm run lint:es && pnpm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepublishOnly": "father doctor && pnpm run build:father"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons-svg": "*4.2.1",
    "@babel/runtime": "^7.22.5",
    "@chakra-ui/icons": "^2.0.19",
    "@chakra-ui/react": "^2.7.1",
    "@chakra-ui/theme-tools": "^2.0.18",
    "@docsearch/css": "^3.5.1",
    "@docsearch/react": "^3.5.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource-variable/inter": "^5.0.3",
    "@makotot/ghostui": "^2.0.0",
    "animated-scroll-to": "^2.3.0",
    "clsx": "^1.2.1",
    "cmdk": "^0.1.22",
    "f2-touchemulator": "*0.1.0",
    "framer-motion": "^7.10.3",
    "heti": "^0.9.4",
    "prism-react-renderer": "^1.3.5",
    "qrcode.react": "*3.1.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-icons": "^4.10.1",
    "umi-hd": "*5.0.1"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@types/react": "^18.2.14",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^18.2.6",
    "@umijs/lint": "^4.0.72",
    "dumi": "^2.3.8",
    "dumi-assets-types": "^2.3.0",
    "eslint": "^8.44.0",
    "father": "^4.2.3",
    "father-plugin-dumi-theme": "1.0.0-rc.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-packagejson": "^2.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stylelint": "^14.16.1"
  },
  "peerDependencies": {
    "dumi": "^2.3.0",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "authors": [
    "artisan<codes.artisan@gmail.com>"
  ]
}
